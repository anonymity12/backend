<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xj.family.mapper.UserLifeMapper">
    <insert id="setLifeStartAndEnd">
        INSERT INTO user_life_start_end_table (user_id, birth, death)
        VALUES (#{userId}, #{start}, #{end})
    </insert>
</mapper>